{
  "name": "Go Pipeline Labelizer",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Go Pipeline Labelizer for Innova",
  "homepage_url": "https://github.com/Gems",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "permissions": [
    "https://go.inn.ru/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://go.inn.ru/*"
      ],
      "js": [
          "src/inject/inject.js"
      ]
    }
  ]
}
